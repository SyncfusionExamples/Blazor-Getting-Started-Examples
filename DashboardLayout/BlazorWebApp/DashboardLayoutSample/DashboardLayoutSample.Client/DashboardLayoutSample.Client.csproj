<Project Sdk="Microsoft.NET.Sdk.BlazorWebAssembly">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <NoDefaultLaunchSettingsFile>true</NoDefaultLaunchSettingsFile>
    <StaticWebAssetProjectMode>Default</StaticWebAssetProjectMode>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="9.0.2" />
    <PackageReference Include="Syncfusion.Blazor.Charts" Version="28.2.12" />
    <PackageReference Include="Syncfusion.Blazor.Grid" Version="28.2.12" />
    <PackageReference Include="Syncfusion.Blazor.Layouts" Version="28.2.12" />
    <PackageReference Include="Syncfusion.Blazor.RangeNavigator" Version="28.2.12" />
    <PackageReference Include="Syncfusion.Blazor.Themes" Version="28.2.12" />
  </ItemGroup>

</Project>
